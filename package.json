{
  "name": "csv",
  "displayName": "CSV",
  "publisher": "ReprEng",
  "description": "Seamlessly Display and Edit CSVs",
  "icon": "images/icon.jpg",
  "repository": {
    "type": "git",
    "url": "https://github.com/jonaraphael/csv.git"
  },
  "version": "1.0.5",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Data Science",
    "Programming Languages",
    "Other"
  ],
  "activationEvents": [
    "onCustomEditor:csv.editor"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "csv.editor",
        "displayName": "CSV",
        "selector": [
          {
            "filenamePattern": "*.csv"
          }
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "vite build",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@types/vscode": "^1.93.0",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3",
    "vite": "^6.0.11"
  }
}